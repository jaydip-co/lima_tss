<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE payara-web-app PUBLIC "-//Payara.fish//DTD Payara Server 4 Servlet 3.0//EN" "https://docs.payara.fish/schemas/payara-web-app_4.dtd">
<payara-web-app error-url="">
  <context-root>/tss</context-root>
  <security-role-mapping>
    <role-name>STUDENT</role-name>
    <group-name>student</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>STAFFMEMBER</role-name>
    <group-name>staffmember</group-name>
  </security-role-mapping>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</payara-web-app>
